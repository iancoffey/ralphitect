{
  "sandbox": {
    "enabled": true,
    "autoAllowBashIfSandboxed": true,
    "allowUnsandboxedCommands": false,
    "filesystem": {
      "denyWrite": ["/"],
      "allowWrite": ["${cwd}"]
    },
    "network": {
      "allowedDomains": [
        "github.com",
        "api.github.com",
        "proxy.golang.org",
        "sum.golang.org",
        "objects.githubusercontent.com"
      ]
    },
    "commands": {
      "allowed": [
        "go",
        "git",
        "make",
        "ls",
        "cat",
        "mkdir",
        "rm",
        "touch",
        "pwd",
        "grep",
        "find",
        "echo",
        "printf",
        "cp",
        "mv",
        "sed",
        "awk",
        "tee",
        "head",
        "tail",
        "chmod",
        "bash",
        "sh"
      ]
    }
  },
  "autoUpdater": { "enabled": false },
  "telemetry": { "enabled": false }
}
